{
  "name": "skillswap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "sync:aliases": "tsx sync-aliases.ts",
    "predev": "npm run sync:aliases",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css,scss}'",
    "format:check": "prettier --check .",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "preview": "vite preview",
    "test": "jest",
    "storybook": "npm run sync:aliases && storybook dev -p 6006",
    "storybook:build": "npm run sync:aliases && storybook build"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ],
    "**/*.{css,scss}": [
      "npm run stylelint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-vitest": "^9.1.2",
    "@storybook/builder-vite": "^9.1.2",
    "@storybook/react": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.2",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.1.5",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "storybook": "^9.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
